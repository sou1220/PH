<div class="container">
  <div class="row">
    <div class="col"><h1><%= current_user.name %>さんの投稿一覧</h1>
      <% @posts.each do |post| %>
        <% if post.user_id == current_user.id %>
          <%= image_tag post.get_image %>
          <p>施設名:<%= post.title %></p>
          <p>コメント:<%= post.body %></p>
          <p>投稿日:<%= post.created_at.strftime('%Y/%m/%d') %></p>
        <% end %>
      <% end %>
    </div>
  </div>
</div>